source 'https://github.com/CocoaPods/Specs.git'

platform :ios, '9.0'

target 'MapMetro' do
  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for MapMetro
  pod 'GoogleMaps'
  pod 'GooglePlaces'

  target 'MapMetroTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'MapMetroUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
